{
  "version": 2,
  "git": {
    "deploymentEnabled": false
  },
  "buildCommand": "cd frontend-svelte && npm install && npm run build && mkdir -p ../.vercel && cp -r .vercel/output ../.vercel/",
  "outputDirectory": ".vercel/output",
  "installCommand": "echo 'skipping root install'",
  "framework": null,
  "build": {
    "env": {
      "PYTHON_INSTALLER": "pip"
    }
  },
  "functions": {
    "api/index.py": {
      "runtime": "@vercel/python@4.3.1",
      "maxDuration": 60
    }
  }
}
